    "Desc": {
      "SC": {
        "Ability": [
          {
            "type": "ptr",
            "value": [
              {"type": "str", "value": "请求"},
              {"type": "str", "value": "$0pt（起始值100％)"},
              {
                "type": "ptr",
                "value": [
                  {"type": "double", "value": 8000},
