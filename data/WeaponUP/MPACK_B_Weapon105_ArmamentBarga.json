    "Desc": {
      "SC": {
        "Ability": [
          {
            "type": "ptr",
            "value": [
              {"type": "str", "value": "再请求"},
              {"type": "str", "value": "$0pt（起始值50％)"},
              {
                "type": "ptr",
                "value": [
                  {"type": "double", "value": 32000},
                  {"type": "double", "value": 21},
                  {"type": "double", "value": 0},
                  {"type": "double", "value": 8},
