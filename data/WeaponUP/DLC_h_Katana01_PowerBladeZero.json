            "value": [
              {"type": "str", "value": "第三擊"},
              {"type": "str", "value": "（傷害:[貫穿]300.0 射程:49.0m）"}
            ]
          },
          {
            "type": "ptr",
            "value": [
              {"type": "str", "value": "揮動速度"},
              {"type": "str", "value": "1.0"}
            ]
          },
          {
            "type": "ptr",
            "value": [
              {"type": "str", "value": "對護盾"},
